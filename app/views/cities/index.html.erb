<h2>Welcome To Destinations Page</h2>
<div class = "cityCount">
There are currently <%= @cities.length %> on our list!
</div>
<% @cities.each do |city| %>
  <div>
    <h3> <%= city.city_name %> </h3>
    <h4> <%= city.country %> </h4>
    <% if current_user %>
      <%= button_to "Add review", reviews_path(city) %>
    <% end %>
  </div>
  <br>
<% end %>
